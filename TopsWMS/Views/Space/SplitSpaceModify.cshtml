@{
    Layout = null;

    string SpaceCode = Request.QueryString["SpaceCode"];
    string SpaceBay = Request.QueryString["SpaceBay"];
    string SpaceLayer = Request.QueryString["SpaceLayer"];
    string Space_Position = Request.QueryString["Space_Position"];
}
<!DOCTYPE html>
<html>
<head>
    <title>储位拆分</title>
    <script src="../../TopsLib/Scripts/jquery-1.5.1.min.js" type="text/javascript"></script>
    <style type="text/css">
        table
        {
            width: 300px;
            height: 160px;
        }
        td:nth-child(odd)
        {
            line-height: 35px;
        }
        td:nth-child(odd)
        {
            line-height: 35px;
            text-align: right;
        }
        input
        {
            border:1px solid skyblue;
        }
    </style>
    <script type="text/javascript">
        function f_getData() {
            var SplitCount = $("[name=SplitCount]").val();
            if (parseInt(SplitCount) <= 0) {
                alert("拆分储位个数必须大于0！请重新输入");
                $("[name=SplitCount]").val("");
                return "";
            }

            return SplitCount;
        }
    </script>
</head>
<body>
    <div>
        <table>
            <tr>
                <td>
                    <span>储位编码:</span>
                </td>
                <td>
                    <input type="text" disabled="disabled" name="SpaceCode" value="@(SpaceCode)" />
                </td>
            </tr>
            <tr>
                <td>
                    <span>所属列:</span>
                </td>
                <td>
                    <input type="text" disabled="disabled" name="SpaceBay" value="@(SpaceBay)" />
                </td>
            </tr>
            <tr>
                <td>
                    <span>所属层:</span>
                </td>
                <td>
                    <input type="text" disabled="disabled" name="SpaceLayer" value="@(SpaceLayer)" />
                </td>
            </tr>
            <tr>
                <td>
                    <span>所在位置:</span>
                </td>
                <td>
                    <input type="text" disabled="disabled" name="Space_Position" value="@(Space_Position)" />
                </td>
            </tr>
            <tr>
                <td>
                    <span>拆分储位个数:</span>
                </td>
                <td>
                    <input type="text" name="SplitCount" />
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
