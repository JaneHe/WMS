                    <div style="width: 40%; height: 90%; display: flex; justify-content: center; align-items: center">
                        <button id="qx_ok" class="button button2">确&nbsp;&nbsp;&nbsp;&nbsp;定</button>
                        <button id="qx_no" class="button button2">取&nbsp;&nbsp;&nbsp;&nbsp;消</button>
                    </div>
                </div>
            </div>
        </div>
        <div style="height: 20px;"></div>
        <div class="l_qx_div1">
            <table class="pure-table" id="table1">
                <thead>
                    <tr style="text-align: center">
                        <th style="width: 20%; text-align: center; font-size: 30px;">#</th>
                        <th style="width: 78%; font-size: 30px;">装箱条码</th>
                    </tr>
                </thead>
                <tbody id="Tables" style="font-size: 25px;">
                </tbody>
            </table>
        </div>
    </div>
</body>
</html>
<script src="../../TopsLib/Scripts/tFronQueentJs.js"></script>
<script type="text/javascript">
    $(function () {
        reloop();  //日期显示
    })


    // 单件流条码回车事件
    $("#cityName").keydown(function (e) {
        if (e.keyCode == 13) {
            var s_PackingASN = $("#cityName").val();
            $("#cityName").val("");
            if (s_PackingASN == "" || s_PackingASN == undefined || s_PackingASN == null) {
                alertN("请扫入条码！");
                $("#cityName").val("");
                $("#cityName").focus().val();
                return;
            }

            for (var i = 0; i < datass.length; i++) {
                if (datass[i] == s_PackingASN) {
                    alertN("该箱号已录入！");
                    return;
                }
            }
            datass[nusberss] = s_PackingASN;

            $.topsAjax({
                type: "GET",
                data: {
                    ASN: s_PackingASN
                },
                url: "/AjaxGet/SeltoReceivingList",
                success: function (data) {
                    //有值赋值
                    //if (data.Data[1].length > 0) {
                    //    alertN("该箱号已扫描请重新扫入!");
                    //    $("#s_PackingASN").val("");
                    //    $("#s_PackingASN").focus().val();
                    //    return;
                    //}
                    //debugger;
                    if (data.Data[0].length > 0) {
                        var tr = ""
                        for (var i = 0; data.Data[0].length > i; i++) {
                            nusberss = nusberss + 1;
                            if (data.Data[0][i].d_OutTime.length == 0) {
                                tr += "<tr name='" + data.Data[0][i].s_PackingASN + "' style='background:#99CCFF;'><td  style='text-align:center'>" + nusberss
                                     + "</td><td style='text-align:center'>" + data.Data[0][i].s_PackingASN
                                      + "</td></tr>"
                            } else {
                                tr += "<tr name='" + data.Data[0][i].s_PackingASN + "' style='background:#29e112;'><td  style='text-align:center'>" + nusberss
                                     + "</td><td style='text-align:center'>" + data.Data[0][i].s_PackingASN
                                      + "</td></tr>"
                            }
                        }
                        $("#Tables").prepend(tr);
                        $("#cityName").focus().val();
                    } else {
                        alertN("该装箱单不存在！");
                        datass = [];
                        $("#cityName").val("");
                        $("#cityName").focus().val();
                    }
                }
            });
        }
    });


    //日期时间显示
    function reloop() {
        var time = new Date();
        var year = time.getFullYear();
        var month = time.getMonth() + 1;
        var day = time.getDate();
        var hour = time.getHours();
        var minute = time.getMinutes();
        var second = time.getSeconds();
        if (minute < 10) {
            minute = "0" + minute;
        }
        if (second < 10) {
            second = "0" + second;
        }
        $("#sp_date").html(year + "/" + month + "/" + day);
        $("#sp_time").html(hour + ":" + minute + ":" + second);
        setTimeout("reloop()", 1000);
    }

    //* 阻止键盘弹出
    function f_preventKeyBoardsss() {
        $("#cityName").click(function () {
            $(this).attr("readonly", "readonly");
            nowfocus = $(this);
            setTimeout(function () {
                nowfocus.removeAttr("readonly");
            }, 100);
        });

        //focus
        $("#cityName").focus(function () {
            $(this).attr("readonly", "readonly");
            nowfocus = $(this);
            setTimeout(function () {
                nowfocus.removeAttr("readonly");
            }, 100);
        });
    }

    //点击切换扫码状态

    var datass = [];
    var nusberss = 0;
    var checkT = true;
    // 封车
    $("#fxBox").click(function () {
        if (checkT == true) {
            checkT = false;

            setTimeout(function () {
                checkT = true
            }, 4000);

            if (datass.length == 0) {
                return;
            }

            var ASN = new Array();

            for (var i = 0; i < datass.length; i++) {
                ASN[i] = {
                    LoadingASN: datass[i]
                }
            }

            $.topsAjax({
                type: "POST",
                url: "/AjaxPost/AddSeltoReceivingList",
                data: { ASN: ASN },
                success: function (data) {
                    if (data.IsSuccess) {
                        alert("出库完成");
                        setTimeout("location.reload();", 4000);
                    }
                }
            });
        } else {
            alert("请勿重复点击");
        }
    });
</script>
