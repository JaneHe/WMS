<script src="../../TopsLib/Scripts/tFronQueentJs.js"></script>
<script type="text/javascript">

    $(function () {
        $("#ZXhao").focus().val();
        reloop();  // 日期显示
        LoadBacth(); // 获取批次
        onDelete(); // 加载缺陷
        f_preventKeyBoardsss();  // 失去键盘事件
    })

    // 获取后装箱单号回车事件
    $("#ZXhao").keydown(function (e) {
        if (e.keyCode == 13) {
            var HouBarcode = $("#ZXhao").val();    // 后箱装箱单
            var Batch = HouBarcode.substring(HouBarcode.indexOf("$") + 1, HouBarcode.indexOf("$") + 7);
            var Label = HouBarcode.substring(0, HouBarcode.indexOf("$"));
            //var ToLabel = HouBarcode.substring(HouBarcode.length - 10, HouBarcode.length);
            var sBatch = $("#Batch").html().trim();


            if (HouBarcode == "") {
                f_commonShowMsgred("后装箱单不能为空！！");
                $("#ZXhao").val("");
                $("#ZXhao").focus().val();
                return;
            }

            if (HouBarcode.indexOf("$") == -1) {
                f_commonShowMsgred("后装箱单没有存在$这个字符！");
                $("#ZXhao").val("");
                $("#ZXhao").focus().val();
                return;
            }

            if (HouBarcode.trim().length <= 14) {
                f_commonShowMsgred("后箱装箱单不能小于14位！！");
                $("#ZXhao").val("");
                $("#ZXhao").focus().val();
                return;
            }

            //if (!num(ToLabel)) {
            //    f_commonShowMsgred("批次流水码必须是纯数字！！");
            //    $("#ZXhao").val("");
            //    $("#ZXhao").focus().val();
            //    return
            //}

            $.topsAjax({
                type: "GET",
                data: { tts: 1, HouBarcode: HouBarcode, Batch: Batch, Label: Label },
                url: "/AjaxGet/SelIsHouBarCode",
                success: function (data) {
                    if (data.Data[0].length > 0) {
                        $("#Product_Label").html(data.Data[0][0].s_Product_Label);
                        $("#s_Name").html(data.Data[0][0].s_Product_Name);
                        $("#ZXhao").attr("disabled", "disabled");

                        if (data.Data[0][0].isOrigin == "2") {
                            $("#ZXhaoOne").val(data.Data[0][0].s_Qian_Label);
                            $("#ZXhaoOne").attr("disabled", "disabled");
                            $("#HouPrint").css({ "background": "#87CEFA" })
                            LoadQianBox(HouBarcode);
                        } else {
                            $("#ZXhaoOne").focus().val();
                            LoadQianBox(HouBarcode);
                        }
                    } else {
                        $("#ZXhao").val("");
                        $("#ZXhao").focus().val();
                        f_commonShowMsgred("该装箱单已封箱，不能在进行扫描！！！");
                    }
                }
            });
        }
    });

    // 获取前装箱单号回车事件
    $("#ZXhaoOne").keydown(function (e) {
        if (e.keyCode == 13) {
            var HouBarcode = $("#ZXhao").val();    // 后箱装箱单
            var QianBarcode = $("#ZXhaoOne").val();    // 前箱装箱单
            //var Label = QianBarcode.substring(0, QianBarcode.indexOf("$"));
            var Label = QianBarcode.substring(0, QianBarcode.lenth - 10);
            var sBatch = $("#Batch").html().trim();

            if (QianBarcode == "") {
                f_commonShowMsgred("前装箱单不能为空！！");
                $("#ZXhaoOne").val("");
                $("#ZXhaoOne").focus().val();
                return;
            }

            if (Label != HouBarcode.substring(0, HouBarcode.indexOf("$"))) {
                f_commonShowMsgred("前装箱单号和后装箱单号不相同,请重新扫码！");
                $("#ZXhaoOne").val("");
                $("#ZXhaoOne").focus().val();
                return;
            }

            if ($("#ZXhao").attr("disabled") == undefined) {
                f_commonShowMsgred("请确认后装箱单！！");
                $("#ZXhaoOne").val("");
                $("#ZXhao").focus().val();
                return;
            }

            //if (!num(ToLabel)) {
            //    f_commonShowMsgred("批次流水码必须是纯数字！！");
            //    $("#ZXhaoOne").val("");
            //    $("#ZXhaoOne").focus().val();
            //    return
            //}

            $.topsAjax({
                type: "GET",
                data: {
                    tts: 1,
                    HouBarcode: HouBarcode, QianBarcode: QianBarcode,
                    sBatch: sBatch, Label: Label
                },
                url: "/AjaxGet/SelIsQianBarCode",
                success: function (data) {
                    if (data.Data.length > 0) {
                        $("#ZXhaoOne").attr("disabled", "disabled");
                        $("#HouPrint").css({ "background": "#87CEFA" });
                        $("#ZXhaoTwo").focus().val();
                        LoadQianBox(HouBarcode);
                    } else {
                        $("#ZXhaoOne").val("");
                        $("#ZXhaoOne").focus().val();
                        f_commonShowMsgred("该前箱装箱单已录入，不能重复录入！！");
                    }
                }
            });
        }
    });

    // 获取前箱工件标签
    $("#ZXhaoTwo").keydown(function (e) {
        if (e.keyCode == 13) {
            var HouBarcode = $("#ZXhao").val();        // 后箱装箱单
            var QianBarcode = $("#ZXhaoOne").val();    // 前箱装箱单
            var Barcode = $("#ZXhaoTwo").val();        // 前箱工件标签
            var Label = Barcode.substring(0, Barcode.indexOf("$"));
            //var ToLabel = Barcode.substring(Barcode.length - 10, Barcode.length);
            var sBatch = $("#Batch").html().trim();    // 当前批次


            if (Barcode == "") {
                f_commonShowMsgred("请先扫描前箱工件码！");
                $("#ZXhaoTwo").focus().val();
                $("#ZXhaoTwo").val("");
                return;
            }

            if (Label != HouBarcode.substring(0, HouBarcode.indexOf("$"))) {
                f_commonShowMsgred("前装箱单号和工件标签不相同,请重新扫码！！");
                $("#ZXhaoTwo").val("");
                $("#ZXhaoOne").focus().val();
                return;
            }

            if ($("#ZXhaoOne").attr("disabled") == undefined) {
                f_commonShowMsgred("请确认前箱装箱单！！");
                $("#ZXhaoOne").focus().val();
                return;
            }

            if (QianBarcode == "") {
                f_commonShowMsgred("请先扫描前箱装箱单！");
                $("#ZXhaoOne").focus().val();
                $("#ZXhaoTwo").val("");
                return;
            }


            //if (!num(ToLabel)) {
            //    f_commonShowMsgred("批次流水码必须是纯数字！！");
            //    $("#ZXhaoTwo").val("");
            //    $("#ZXhaoTwo").focus().val();
            //    return
            //}

            $.topsAjax({
                type: "GET",
                data: {
                    tts: 2,
                    Barcode: Barcode
                },
                url: "/AjaxGet/SelIsHouNumber",
                success: function (data) {
                    if (data.Data[0][0].num == 0) {
                        $.topsAjax({
                            type: "Post",
                            url: "/AjaxPost/AddIsHouCodeList",
                            data: { Barcode: Barcode, QianBarcode: QianBarcode, Label: Label, sBatch: sBatch, HouBarcode: HouBarcode },
                            success: function (Addnum) {
                                if (Addnum.IsSuccess) {
                                    $("#ZXhaoTwo").val("");
                                    $("#ZXhaoTwo").focus().val();
                                    if (Addnum.OutPut.QualityNum == "0") {
                                        alertN("已达到标准装箱数量，请确认封箱！！");
                                        $.topsAjax({
                                            type: "GET",
                                            data: {
                                                tts: 3,
                                                HouBarcode: HouBarcode
                                            },
                                            url: "/AjaxGet/SelIsHouBarCode",
                                            success: function (data) {
                                                if (data.IsSuccess) {
                                                    if (data.Data.length > 0) {
                                                        f_commonShowMsg("封箱完成！！");
                                                        setTimeout("location.reload();", 3000)
                                                    }
                                                }
                                            }
                                        })
                                    } else {
