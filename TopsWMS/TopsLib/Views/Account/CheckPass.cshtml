@{ 
    string Email = Request.QueryString["Email"].ToString();
    string IsCheck = Request.QueryString["IsCheck"].ToString();
    string UserNO = Request.QueryString["UserNO"].ToString();
    string UserName = Request.QueryString["UserName"].ToString();

    string Msgs = ViewBag.Msgs;
    
}
<!DOCTYPE html>
<html>
<head>
    <title>完成审核</title>
    <style type="text/css">
        body
        {
            position: absolute;
            width: 100%;
            height: 100%;
        }
        .message
        {
            position: relative;
            font-family: 微软雅黑;
            font-size: 20px;
            width: calc(50%);
            line-height: 60px;
            font-weight: bold;
            background: #d3dade40;
            border-radius: 10px;
            display: inline-block;
            cursor: pointer;
            border: 1px solid black;
            top: calc(20%);
            left: calc(20%);
        }
        .cricle
        {
            display: inline-block;
            background: #19ba19;
            border-radius: 100px;
            width: 40px;
            height: 40px;
            color: white;
            font-weight: bold; 
            text-align: center;
        }
        .cricle_span
        {
            border-radius: 100px;
            width: 40px;
            height: 40px;
            line-height: 40px;
        }
        .cricle_result
        {
            display: inline-block;
        }
    </style>
    <script src="../../../Scripts/jquery-1.4.4.min.js" type="text/javascript"></script>
    <script type="text/javascript">
        var alertmessage = "已"+"@(IsCheck.Equals("1") ? "通过" : "拒绝")"+"申请人"+"@(UserName)"+"的申请请求,结果将以邮件方式通知到申请人";
        var IsCheck = "@(IsCheck)"; 
        var Msgs = "@(Msgs)";

        $(document).ready(function () {
             
        });
    </script>
</head>
<body>
    <div class="message">
        <div class="cricle">
            <div class="cricle_span">
                √</div>
        </div>
        @if (string.IsNullOrEmpty(Msgs))
        {
            <div class="cricle_result">
                已@(IsCheck.Equals("1") ? "通过" : "拒绝")申请人@(UserName)的申请请求,结果将以邮件方式通知到申请人</div> 
        }
        else
        { 
            <div class="cricle_result">@(Msgs)</div> 
        
        }
    </div>
</body>
</html>
