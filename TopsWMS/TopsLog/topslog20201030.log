  and n_ReportInventoryCurrent_state = (case isnull(@n_ReportInventoryCurrent_state,0) when 0
  then n_ReportInventoryCurrent_state else @n_ReportInventoryCurrent_state end)
and [n_Space_Id] = '031B53F3-5EB8-4A5C-91DC-09EE2158BAC9'
 ) T_pagenation  ) TT_pagenation  where RowNumber between @beginIndex and @endIndex 



