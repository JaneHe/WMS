  FROM [dbo].[tReason] with(nolock)
where s_Reason_Name like (case when isnull(@Reason_Name,'') = '' then s_Reason_Name else '%'+@Reason_Name+'%' end) 
and n_Reason_state = (case when isnull(@Reason_state,0) = 0 then n_Reason_state else @Reason_state end)
) T_pagenation  ) TT_pagenation  where RowNumber between @beginIndex and @endIndex 
 



