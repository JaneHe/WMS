      when 3 then '报废'
      when 4 then '返工'
      else '' end) as [n_BillStockInOut_QualityStatus]
      ,[s_BillStockInOut_Operator]
      ,[s_BillStockInOut_Department]
      ,(case [s_BillStockInOut_Team] when 1 then '白班'
