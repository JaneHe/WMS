select 
--[Columns]
    UserNO=s_UserNO,
    UserName=s_UserName,
    State=n_State,
    ComputerName=s_ComputerName,
    JobNumber= s_JobNumber,
    BanCi=s_BanCi,
    RoleID=tRole.s_RoleName,
    WorkingProcedure=s_WorkingProcedure,
    QRCode=s_QRCode,
    Sex=case isnull(n_Sex,-1) when -1 then '' when 0 then '女' else '男' end,
    DepartmentName=s_DepartmentName,
    ContactWay=s_ContactWay,

    Address=s_Address,
    EntryDate=d_EntryDate,
    RegularDate=d_RegularDate,
    DimissionDate=d_DimissionDate,
    Birthday=d_Birthday, 
    Remarks=s_Remarks 
    --[/Columns]
from tUser tu with(nolock)
inner join tUserRole tur with(nolock) on tur.s_UserID = tu.s_UserNO  
inner join tRole tRole with(nolock) on tRole.s_RoleID = tur.s_RoleID
and ltrim(rtrim(tur.s_RoleID)) <> '001'
where n_State = 1 and s_GroupType = '2' 
and ISNULL(s_UserName,'')   like case ISNULL(@UserName,'')  when '' then ISNULL(s_UserName,'')  else '%'+@UserName+'%' end
and ISNULL(tu.s_JobNumber,'')  like case ISNULL(@JobNumber,'') when '' then ISNULL(tu.s_JobNumber,'') else '%'+@JobNumber+'%' end
and ISNULL(tur.s_RoleID,'') = case ISNULL(@RoleName,'') when '' then  ISNULL(tur.s_RoleID,'')   else @RoleName end 
 
group by s_UserNO,s_UserName,n_State,s_ComputerName, s_JobNumber,s_BanCi,
tur.s_RoleID,s_WorkingProcedure,s_QRCode,n_Sex,s_DepartmentName,d_EntryDate,
d_RegularDate,d_DimissionDate,s_Remarks,s_ContactWay,d_Birthday,s_Address,tRole.s_RoleName
) T_pagenation  ) TT_pagenation  where RowNumber between @beginIndex and @endIndex 
