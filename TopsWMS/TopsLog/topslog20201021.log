      ,[s_ReportInventoryCurrent_SpecificationAndModel]
      ,[s_ReportInventoryCurrent_Unit]
      ,[s_ReportInventoryCurrent_Model]
      ,[s_ReportInventoryCurrent_Color] 
      ,[n_ReportInventoryCurrent_state]
      ,[s_ReportInventoryCurrent_Creator]
      ,[d_ReportInventoryCurrent_CreateTime]
      ,[s_ReportInventoryCurrent_Updator]
      ,[d_ReportInventoryCurrent_UpdateTime]
--[/Columns]
  FROM [dbo].[tReportInventoryCurrent] tReportInventoryCurrent with(nolock)
   where s_ReportInventoryCurrent_Code like (case when ISNULL(@s_ReportInventoryCurrent_Code,'') = ''
  then s_ReportInventoryCurrent_Code else '%'+@s_ReportInventoryCurrent_Code+'%' end) 
  and [s_ReportInventoryCurrent_GoodsProductCode] like (case when ISNULL(@s_ReportInventoryCurrent_GoodsProductCode,'') = ''
  then [s_ReportInventoryCurrent_GoodsProductCode] else '%'+@s_ReportInventoryCurrent_GoodsProductCode+'%' end)
  and [s_ReportInventoryCurrent_GoodsProductName] like (case when ISNULL(@s_ReportInventoryCurrent_GoodsProductName,'') = ''
  then [s_ReportInventoryCurrent_GoodsProductName] else '%'+@s_ReportInventoryCurrent_GoodsProductName+'%' end)
  and n_ReportInventoryCurrent_state = (case isnull(@n_ReportInventoryCurrent_state,0) when 0
  then n_ReportInventoryCurrent_state else @n_ReportInventoryCurrent_state end)
and [n_Space_Id] = '322A95CB-3CDC-4B7B-A82F-B86C1593435F'
 ) T_pagenation  ) TT_pagenation  where RowNumber between @beginIndex and @endIndex 



