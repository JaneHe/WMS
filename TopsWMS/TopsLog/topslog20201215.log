说明：执行查询对象查询(1)
***************************************************************************************************************
SQL:
select * from ( select RowNumber=row_number() over(order by RecordInventoryMoveCount_Time asc),* from (
select
--[Columns]
    
	RecordInventoryMoveCount_UserName = tUser.s_UserName,
    RIMC_ReportInventoryCurrent_Code=n_RIMC_ReportInventoryCurrent_Code,  
    BillStockInOut_Code=s_BillStockInOut_Code,
    RecordInventoryMoveCount_GoodsProductName=s_RecordInventoryMoveCount_GoodsProductName,
    RecordInventoryMoveCount_GoodsProductCode=s_RecordInventoryMoveCount_GoodsProductCode,
    RecordInventoryMoveCount_ExternalCode=s_RecordInventoryMoveCount_ExternalCode,
    RecordInventoryMoveCount_ECN=s_RecordInventoryMoveCount_ECN,  
    RecordInventoryMoveCount_QualityStatus=(case n_RecordInventoryMoveCount_QualityStatus 
	when 1 then '合格' when 2 then '不合格' when 3 then '报废' when 4 then '返工' else '' end),
    RecordInventoryMoveCount_SpecificationAndModel=s_RecordInventoryMoveCount_SpecificationAndModel,
    RecordInventoryMoveCount_Unit=s_RecordInventoryMoveCount_Unit,
    RecordInventoryMoveCount_Model=s_RecordInventoryMoveCount_Model,
    RecordInventoryMoveCount_Color=s_RecordInventoryMoveCount_Color,
    RecordInventoryMoveCount_BatchNumber=s_RecordInventoryMoveCount_BatchNumber,
    RecordInventoryMoveCount_Number=n_RecordInventoryMoveCount_Number,
    RecordInventoryMoveCount_OKProductCou=n_RecordInventoryMoveCount_OKProductCou,
    RecordInventoryMoveCount_UnOKProductCou=n_RecordInventoryMoveCount_UnOKProductCou,
    RecordInventoryMoveCount_ReworkProductCou=n_RecordInventoryMoveCount_ReworkProductCou,    
    RIMC_ReportInventoryCurrent_Number=n_RIMC_ReportInventoryCurrent_Number,
    RIMC_ReportInventoryCurrent_OKProductCou=n_RIMC_ReportInventoryCurrent_OKProductCou,
    RIMC_ReportInventoryCurrent_UnOKProductCou=n_RIMC_ReportInventoryCurrent_UnOKProductCou,
    RIMC_ReportInventoryCurrent_ReworkProductCou=n_RIMC_ReportInventoryCurrent_ReworkProductCou,
    
    RecordInventoryMoveCount_Time=n_RecordInventoryMoveCount_Time,
    RecordInventoryMoveCount_AreDifferencesCorrected=n_RecordInventoryMoveCount_AreDifferencesCorrected,
    
    RecordInventoryMoveCount_Type=(case n_RecordInventoryMoveCount_Type when 1 then '旧FIFO方式异动盘点'
	when 2 then '新异动盘点' else '' end),
    RecordInventoryMoveCount_WhetherToSubmit=(case isnull(n_RecordInventoryMoveCount_WhetherToSubmit,0)
	when 0 then '否' when 1 then '是' else '否' end),
    RecordInventoryMoveCount_state=(case isnull(n_RecordInventoryMoveCount_state,0) when 1 then '正常'
	else '停用' end) 
--[/Columns]
from tRecordInventoryMoveCount tRecordInventoryMoveCount with(nolock) 
inner join tUser tUser with(nolock) on tUser.s_UserNO = tRecordInventoryMoveCount.s_RecordInventoryMoveCount_Creator
where 
n_RecordInventoryMoveCount_Time >= (case when isnull(@BeginTime,'1900-01-01') = '1900-01-01' then 
n_RecordInventoryMoveCount_Time else @BeginTime end)
and n_RecordInventoryMoveCount_Time <= (case when isnull(@EndTime,'2900-01-01') = '2900-01-01' then 
n_RecordInventoryMoveCount_Time else @EndTime end)
and n_RIMC_ReportInventoryCurrent_Code like (case when isnull(n_RIMC_ReportInventoryCurrent_Code,'') = ''
then n_RIMC_ReportInventoryCurrent_Code else '%'+@n_RIMC_ReportInventoryCurrent_Code+'%' end)
and s_BillStockInOut_Code like (case when isnull(s_BillStockInOut_Code,'') = ''
then s_BillStockInOut_Code else '%'+@s_BillStockInOut_Code+'%' end)
and tUser.s_UserName like (case when isnull(@s_UserName,'') = ''
then tUser.s_UserName else '%'+@s_UserName+'%' end)
and s_RecordInventoryMoveCount_GoodsProductCode like (case when isnull(s_RecordInventoryMoveCount_GoodsProductCode,'') = ''
then s_RecordInventoryMoveCount_GoodsProductCode else '%'+@s_RecordInventoryMoveCount_GoodsProductCode+'%' end)
and s_RecordInventoryMoveCount_GoodsProductName like (case when isnull(s_RecordInventoryMoveCount_GoodsProductName,'') = ''
then s_RecordInventoryMoveCount_GoodsProductName else '%'+@s_RecordInventoryMoveCount_GoodsProductName+'%' end)
and n_RecordInventoryMoveCount_state = (case when isnull(@n_RecordInventoryMoveCount_state,0) = 0
