SQL:
 
select
  
    n_RIMC_ReportInventoryCurrent_Code as '储位二维码',  
    s_BillStockInOut_Code as '单据编码',
    s_RecordInventoryMoveCount_GoodsProductName as '货物产品名称',
    s_RecordInventoryMoveCount_GoodsProductCode as '货物产品编码',
    s_RecordInventoryMoveCount_ExternalCode as 'U9系统编码',
    s_RecordInventoryMoveCount_ECN as '工程变更号',  
    (case n_RecordInventoryMoveCount_QualityStatus 
	when 1 then '合格' when 2 then '不合格' when 3 then '报废' when 4 then '返工' else '' end) as '质量状态',
    s_RecordInventoryMoveCount_SpecificationAndModel as '规格型号',
    s_RecordInventoryMoveCount_Unit as '单位',
    s_RecordInventoryMoveCount_Model as '车型',
    s_RecordInventoryMoveCount_Color as '颜色',
    s_RecordInventoryMoveCount_BatchNumber as '批次号',
    n_RecordInventoryMoveCount_Number as '库位数量',
    n_RecordInventoryMoveCount_OKProductCou as '库位合格数',
    n_RecordInventoryMoveCount_UnOKProductCou as '库位不合格数',
    n_RecordInventoryMoveCount_ReworkProductCou as '库位返工数',    
    n_RIMC_ReportInventoryCurrent_Number as '盘点数量',
    n_RIMC_ReportInventoryCurrent_OKProductCou as '盘点合格数',
    n_RIMC_ReportInventoryCurrent_UnOKProductCou as '盘点不合格数',
    n_RIMC_ReportInventoryCurrent_ReworkProductCou as '盘点返工数',
    tUser.s_UserName as '操作员',
    
    n_RecordInventoryMoveCount_Time as '盘点时间',
    n_RecordInventoryMoveCount_AreDifferencesCorrected as '是否已经纠正',
    
    (case n_RecordInventoryMoveCount_Type when 1 then '旧FIFO方式异动盘点'
	when 2 then '新异动盘点' else '' end) as '盘点方式',
    (case isnull(n_RecordInventoryMoveCount_WhetherToSubmit,0)
	when 0 then '否' when 1 then '是' else '否' end) as '是否已经提交',
    (case isnull(n_RecordInventoryMoveCount_state,0) when 1 then '正常'
	else '停用' end) as  '状态'
 
from tRecordInventoryMoveCount tRecordInventoryMoveCount with(nolock) 
inner join tUser tUser with(nolock) on tUser.s_UserNO = tRecordInventoryMoveCount.s_RecordInventoryMoveCount_Creator
where 
n_RecordInventoryMoveCount_Time >= (case when isnull(@BeginTime,'1900-01-01') = '1900-01-01' then 
n_RecordInventoryMoveCount_Time else @BeginTime end)
and n_RecordInventoryMoveCount_Time <= (case when isnull(@EndTime,'') = '' then 
n_RecordInventoryMoveCount_Time else @EndTime end)
and n_RIMC_ReportInventoryCurrent_Code like (case when isnull(n_RIMC_ReportInventoryCurrent_Code,'') = ''
then n_RIMC_ReportInventoryCurrent_Code else '%'+@n_RIMC_ReportInventoryCurrent_Code+'%' end)
and s_BillStockInOut_Code like (case when isnull(s_BillStockInOut_Code,'') = ''
then s_BillStockInOut_Code else '%'+@s_BillStockInOut_Code+'%' end)
and tUser.s_UserName like (case when isnull(@s_UserName,'') = ''
then tUser.s_UserName else '%'+@s_UserName+'%' end)
and s_RecordInventoryMoveCount_GoodsProductCode like (case when isnull(s_RecordInventoryMoveCount_GoodsProductCode,'') = ''
then s_RecordInventoryMoveCount_GoodsProductCode else '%'+@s_RecordInventoryMoveCount_GoodsProductCode+'%' end)
and s_RecordInventoryMoveCount_GoodsProductName like (case when isnull(s_RecordInventoryMoveCount_GoodsProductName,'') = ''
then s_RecordInventoryMoveCount_GoodsProductName else '%'+@s_RecordInventoryMoveCount_GoodsProductName+'%' end)
and n_RecordInventoryMoveCount_state = (case when isnull(@n_RecordInventoryMoveCount_state,0) = 0
then n_RecordInventoryMoveCount_state else @n_RecordInventoryMoveCount_state end)
and n_RecordInventoryMoveCount_WhetherToSubmit = (case when isnull(@n_RecordInventoryMoveCount_WhetherToSubmit,0) = 0
then n_RecordInventoryMoveCount_WhetherToSubmit else @n_RecordInventoryMoveCount_WhetherToSubmit end)
and n_RecordInventoryMoveCount_Number < n_RIMC_ReportInventoryCurrent_Number
 
