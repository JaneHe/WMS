n_RecordInventoryMoveCount_Time else @BeginTime end)
and n_RecordInventoryMoveCount_Time <= (case when isnull(@EndTime,'2900-01-01') = '2900-01-01' then 
n_RecordInventoryMoveCount_Time else @EndTime end)
and n_RIMC_ReportInventoryCurrent_Code like (case when isnull(n_RIMC_ReportInventoryCurrent_Code,'') = ''
then n_RIMC_ReportInventoryCurrent_Code else '%'+@n_RIMC_ReportInventoryCurrent_Code+'%' end)
and s_BillStockInOut_Code like (case when isnull(s_BillStockInOut_Code,'') = ''
then s_BillStockInOut_Code else '%'+@s_BillStockInOut_Code+'%' end) 
and s_RecordInventoryMoveCount_GoodsProductCode like (case when isnull(s_RecordInventoryMoveCount_GoodsProductCode,'') = ''
then s_RecordInventoryMoveCount_GoodsProductCode else '%'+@s_RecordInventoryMoveCount_GoodsProductCode+'%' end)
and s_RecordInventoryMoveCount_GoodsProductName like (case when isnull(s_RecordInventoryMoveCount_GoodsProductName,'') = ''
then s_RecordInventoryMoveCount_GoodsProductName else '%'+@s_RecordInventoryMoveCount_GoodsProductName+'%' end)
and n_RecordInventoryMoveCount_state = (case when isnull(@n_RecordInventoryMoveCount_state,0) = 0
then n_RecordInventoryMoveCount_state else @n_RecordInventoryMoveCount_state end)
and n_RecordInventoryMoveCount_WhetherToSubmit = (case when isnull(@n_RecordInventoryMoveCount_WhetherToSubmit,0) = 0
then n_RecordInventoryMoveCount_WhetherToSubmit else @n_RecordInventoryMoveCount_WhetherToSubmit end)
and n_RecordInventoryMoveCount_Number < n_RIMC_ReportInventoryCurrent_Number
) T_pagenation  ) TT_pagenation  where RowNumber between @beginIndex and @endIndex 
