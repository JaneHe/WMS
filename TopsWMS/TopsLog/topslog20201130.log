      ,[s_PartType]
      ,[s_PartType_Desc] 
      ,[s_PlasticParts_CustomerCode] 
) tPlasticPart_PBOM on Infotable.s_BillStockInOut_ExternalCode
 = tPlasticPart_PBOM.s_PlasticParts_Code 
order by f_BillStockInOut_WarehousingWorkOrderQuantity desc 

 




 
