     s_MotherCode,
     s_Mother_Desc,  
	 s_CarColor,
	 s_CarBrand,
	 s_CarSeries,
	 s_CarType,
	 s_Batch,
	 s_SurplusAmount,
	 n_ReportInventoryCurrent_Id,  
	 n_ReportInventoryCurrentDetail_Id,
	 s_Space_Code,
	 s_Bin_Code
 )
