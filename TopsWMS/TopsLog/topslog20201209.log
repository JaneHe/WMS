	 s_CarSeries varchar(max),
	 s_CarType varchar(max),
	 s_Batch varchar(max), 
	 n_ReportInventoryCurrent_Id varchar(max),  
	 n_ReportInventoryCurrentDetail_Id varchar(max),  
	 s_Space_Code varchar(max),
	 s_Bin_Code varchar(max),
	 s_SurplusAmount int
 )

 

 declare @n_ReportInventoryCurrent_Number int 
 declare @n_ReportInventoryCurrent_Id varchar(max) 
 declare @s_ReportInventoryCurrentDetail_BatchNumber varchar(max)
 declare @GoodsBarCode varchar(max)
 declare @s_Space_Code varchar(max)
 declare @s_Bin_Code varchar(max)

 select top 1
 @n_ReportInventoryCurrent_Id = n_ReportInventoryCurrent_Id, 
 @n_ReportInventoryCurrent_Number = n_ReportInventoryCurrentDetail_Number,
 @s_ReportInventoryCurrentDetail_BatchNumber=s_ReportInventoryCurrentDetail_BatchNumber,
 @GoodsBarCode = s_ReportInventoryCurrentDetail_GoodsProductCode,
 @s_Space_Code = s_Space_BarCode,
 @s_Bin_Code = s_Bin_Code
 from tReportInventoryCurrentDetail tReportInventoryCurrentDetail with(nolock)
 inner join tSpace tSpace with(nolock) on tSpace.n_Space_Id = tReportInventoryCurrentDetail.n_Space_Id
 left join tBin tBin with(nolock) on tBin.n_Bin_Id = tReportInventoryCurrentDetail.n_Bin_Id
 where n_ReportInventoryCurrentDetail_Id = @n_ReportInventoryCurrentDetail_Id 


 declare @Month varchar(20)
 select top 1 @Month = s_Month from TopsERP_BIZ.dbo.tPBOM with(nolock) where n_State = 1 order by replace(s_Month,'-','') desc 

