select 
n_ReportInventoryCurrentDetail_Id,
s_ReportInventoryCurrentDetail_Code,
s_ReportInventoryCurrentDetail_GoodsProductCode,
s_ReportInventoryCurrentDetail_GoodsProductName=(case when len(s_ReportInventoryCurrentDetail_GoodsProductName)>30 then SUBSTRING(s_ReportInventoryCurrentDetail_GoodsProductName,0,30)+ '......' else s_ReportInventoryCurrentDetail_GoodsProductName end), 
s_Bin_Code,
s_ReportInventoryCurrentDetail_BatchNumber,
n_ReportInventoryCurrentDetail_Number 

from tReportInventoryCurrentDetail tReportInventoryCurrentDetail with(nolock)
left join tBin tBin with(nolock) on tBin.n_Bin_Id = tReportInventoryCurrentDetail.n_Bin_Id
inner join tSpace tSpace with(nolock) on tSpace.n_Space_Id = tReportInventoryCurrentDetail.n_Space_Id
where s_ReportInventoryCurrentDetail_QualityStatus = @s_ReportInventoryCurrentDetail_QualityStatus
and s_ReportInventoryCurrentDetail_Code = (case isnull(@s_ReportInventoryCurrentDetail_Code,'')
when '' then s_ReportInventoryCurrentDetail_Code else @s_ReportInventoryCurrentDetail_Code end)
and n_ReportInventoryCurrentDetail_state = 1
order by n_Space_ShipmentSequence asc,s_ReportInventoryCurrentDetail_Code asc
 
