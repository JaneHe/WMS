,@n_ReportInventoryCurrent_Id
,@n_ReportInventoryCurrentDetail_Id,
@s_Space_Code,@s_Bin_Code
from (
select  
s_MotherCode,s_Mother_Desc,s_PartType,s_Color,s_Brand,s_Series 
