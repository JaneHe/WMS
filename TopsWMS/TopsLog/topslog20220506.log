      ,[s_SysParameter_CustomFields8]
      ,[s_SysParameter_CustomFields9]
      ,[s_SysParameter_CustomFields10]
      ,[n_SysParameter_state]=(case n_SysParameter_state when 1 then '正常' else '停用' end)
      ,[s_SysParameter_Creator]
      ,[d_SysParameter_CreateTime]
      ,[s_SysParameter_Updator]
      ,[d_SysParameter_UpdateTime]
--[/Columns]
  FROM [TopsWMS_BIZ].[dbo].[tSysParameter] with(nolock)
where [s_SysParameter_Name] like (case when isnull(@s_SysParameter_Name,'') = '' then [s_SysParameter_Name] else '%'+@s_SysParameter_Name+'%' end)
) T_pagenation  ) TT_pagenation  where RowNumber between @beginIndex and @endIndex 
 



