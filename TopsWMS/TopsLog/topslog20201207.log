 inner join tReportInventoryCurrent tReportInventoryCurrent with(nolock) on tab.s_MotherCode = tReportInventoryCurrent.s_ReportInventoryCurrent_GoodsProductCode
 where n_ReportInventoryCurrent_state = 1
